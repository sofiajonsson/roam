<%= form_for @user do |f| %>
	<%= f.label :username, "Input your Username:" %>
	<%= f.text_field :username %><br>
	<%= f.label :password, "Input your Password:" %>
	<%= f.password_field :password %><br>
	<%= f.label :bio %>
	<%= f.text_area :bio %>
	<%= f.label :photo_url %>
	<%= f.text_field :photo_url %>
	<%= f.submit "Submit" %>
<% end %>
